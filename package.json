{
  "name": "elm-minesweeper",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.com/stephl001/elm-minesweeper.svg?branch=master)](https://travis-ci.com/stephl001/elm-minesweeper)",
  "main": "elm.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "chokidar-cli": "^1.2.1",
    "elm": "^0.19.0-bugfix6"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "chokidar \"src/*.elm\" -c \"node_modules\\elm\\unpacked_bin\\elm.exe make src/Main.elm --output main.compiled.js\" --initial",
    "release": "node_modules\\elm\\unpacked_bin\\elm.exe make src/Main.elm --optimize --output main.compiled.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephl001/elm-minesweeper.git"
  },
  "author": "Stephane Larocque",
